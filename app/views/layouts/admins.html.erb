<!DOCTYPE html>
<html>
  <head>
    <title><%= yield :title %></title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag "admins/admins", media: "all", "data-turbolinks-track": "reload" %>
    <%= javascript_pack_tag "admins/font-awesome-all", "data-turbolinks-track": "reload" %>
  </head>
  <body class="sb-nav-fixed">
    <%= render "admins/layouts/navbar" %>
    <div id="layoutSidenav">
      <%= render "admins/layouts/sidenav" %>
      <div id="layoutSidenav_content">
        <%= yield %>
        <%= render "admins/layouts/footer" %>
      </div>
    </div>
    <%= javascript_pack_tag "admins", "data-turbolinks-track": "reload" %>
  </body>
</html>